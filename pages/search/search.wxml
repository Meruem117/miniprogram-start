<view class="search-row">
  <image src="/icon/search.png" class="search-img"></image>
  <input typpe="text" placeholder="搜素" confirm-type="search" bindinput="sinput" bindconfirm="sconfirm"></input>
  <button bindtap="returnToIndex">取消</button>
</view>

<scroll-view scroll-y class="plus" bindscrolltoupper="upper" upper-threshold="10" lower-threshold="5" bindscrolltolower="lower" 
 scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}">

<view class="search-content">
  <view class="search-item" wx:for="{{feed}}" wx:for-item="item">
    <view class="search_img" bindtap="toUser" data-uid="{{item.user_id}}">
      <image src="{{item.user_img}}"></image>
    </view>
    <text bindtap="check" data-cid="{{item.info_id}}">{{item.content}}</text>
  </view>
</view>

</scroll-view>